# ThingWorx Configuration
# Copy this file to .env and replace with your actual credentials
# NEVER commit .env to version control

THINGWORX_APP_KEY=your-app-key-here
THINGWORX_BASE_URL=http://your-server:port/Thingworx

# LLM Configuration (Local Ollama)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.1:8b

# RAG Configuration
USE_LOCAL_RAG=true
